<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="1342" author="Alexandr Hapon">
        <renameTable  catalogName="cat"
                      newTableName="orders"
                      oldTableName="order"
                      schemaName="public"/>
    </changeSet>

    <changeSet author="Alexandr Hapon" id="1">
        <addAutoIncrement
                          columnDataType="int"
                          columnName="id"
                          defaultOnNull="false"
                          generationType="ALWAYS"
                          incrementBy="1"
                          startWith="1"
                          tableName="orders"/>
    </changeSet>

    <changeSet id="124" author="Aleksandr Hapon">
        <renameTable oldTableName="order_meals" newTableName="orders_meals"/>
    </changeSet>
    <changeSet author="liquibase-docs" id="dropColumn-example">
        <dropColumn catalogName="cat"
                    columnName="date"
                    schemaName="public"
                    tableName="meal">
            <column name="date"/>
        </dropColumn>
    </changeSet>

</databaseChangeLog>
