<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="1" author="Alexandr Hapon" labels="example-label" context="example-context">
        <comment>example-comment</comment>
        <createTable tableName="account">
            <column name="id" type="int" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="money" type="decimal(38,2)"/>
            <column name="name" type="varchar(255)"/>
            <column name="phone_number" type="varchar(255)"/>
        </createTable>
    </changeSet>

    <changeSet id="2" author="Alexandr Hapon" labels="example-label" context="example-context">
        <comment>example-comment</comment>
        <createTable tableName="meal">
            <column name="id" type="int" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="date" type="varchar(255)"/>
            <column name="name" type="varchar(255)"/>
            <column name="price" type="decimal(38,2)"/>
            <column name="time" type="int"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
